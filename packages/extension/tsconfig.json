{
  "compilerOptions": {
    "outDir": "./built",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "allowJs": true,
    "types": [
      "chrome",
      "node"
    ],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
