{
  "name": "solary",
  "private": true,
  "version": "1.9.1",
  "description": "Extension navigateur pour la WebTV Solary",
  "author": "Hugo Alliaume <kocal@live.fr>",
  "license": "MIT",
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "test:e2e": "start-server-and-test test:e2e:_start-server http://localhost:5000 test:e2e:_run",
    "test:e2e:ci": "start-server-and-test test:e2e:_start-server http://localhost:5000 test:e2e:_run:record",
    "test:e2e:_start-server": "yarn serve dist -p 5000",
    "test:e2e:_run": "yarn cypress run",
    "test:e2e:_run:record": "yarn cypress run --record",
    "prettier": "prettier '{src,__tests__}/**/*.{ts,vue}'",
    "prettier:write": "npm run prettier -- --write",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kocal/Solary.git"
  },
  "bugs": {
    "url": "https://github.com/Kocal/Solary/issues"
  },
  "homepage": "https://github.com/Kocal/Solary#readme",
  "dependencies": {
    "@kocal/web-extension-library": "^1.0.0",
    "vue": "^2.5.17",
    "vue-awesome": "^2.3.5",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chrome": "^0.0.74",
    "@types/node": "^10.11.7",
    "babel-core": "^7.0.0-bridge.0",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.10",
    "cypress": "^3.1.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "start-server-and-test": "^1.7.4",
    "ts-loader": "^5.2.1",
    "typescript": "^3.1.3",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-shell-plugin": "^0.5.0",
    "zip-folder": "^1.0.0"
  }
}
